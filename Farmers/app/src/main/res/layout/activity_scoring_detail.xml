                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="5分"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/dp60"
                    android:textColor="@color/txt_black_51"
                    android:textSize="@dimen/sp18"
                    />
                <ImageView
                    android:layout_width="@dimen/dp23"
                    android:layout_height="@dimen/dp23"
                    android:text="@string/pic"
                    android:layout_gravity="center_vertical"
                    android:textColor="@color/txt_black_51"
                    android:gravity="center_vertical"
                    android:background="@drawable/bg_pic_has"
                    android:layout_marginLeft="@dimen/dp48"
                    android:textSize="@dimen/sp18"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="3分"
                    android:gravity="center_vertical"
                    android:textColor="@color/txt_black_51"
                    android:textSize="@dimen/sp18"
                    android:layout_marginLeft="@dimen/dp45"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp48"
                android:orientation="horizontal"
                >
