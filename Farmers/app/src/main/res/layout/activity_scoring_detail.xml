                    android:gravity="center_vertical"
                    android:textSize="@dimen/sp16"
                    android:textColor="@color/txt_black_51"
                    android:layout_marginLeft="@dimen/dp38"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
