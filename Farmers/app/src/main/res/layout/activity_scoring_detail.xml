                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="3分"
                    android:gravity="center_vertical"
                    android:textColor="@color/txt_black_51"
                    android:textSize="@dimen/sp18"
                    android:layout_marginLeft="@dimen/dp45"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp59"
                android:orientation="horizontal"
                android:background="@color/color_00d39d"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/amount"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/sp16"
                    android:textColor="@color/write"
                    android:layout_marginLeft="@dimen/dp38"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="5分"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/dp60"
